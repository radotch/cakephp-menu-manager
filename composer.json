{
    "name": "radotch/cakephp-menu-manager",
    "description": "MenuManager plugin for CakePHP",
    "type": "cakephp-plugin",
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslav Cholakov",
            "email": "tcholakov_radoslav@yahoo.com",
            "homepage": "http://rado.etnasoft.eu",
            "role": "Developer"
        }
    ],
    "require": {
        "cakephp/cakephp": "^3.5"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.7.14|^6.0"
    },
    "autoload": {
        "psr-4": {
            "MenuManager\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "MenuManager\\Test\\": "tests/",
            "Cake\\Test\\": "vendor/cakephp/cakephp/tests/"
        }
    }
}